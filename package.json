  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./formatters": "./dist/formatters.js"
  },
  "dependencies": {
    "type-fest": "^4.9.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.5.3",
    "testcontainers": "^10.9.0",
    "vitest": "^1.5.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "type-fest": "^4.9.0"
  },
  "scripts": {
    "generate": "npx openapi-typescript https://petstore3.swagger.io/api/v3/openapi.json --output ./openapi/petstore.ts",
